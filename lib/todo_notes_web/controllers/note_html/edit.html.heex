<Layouts.app flash={@flash} current_scope={@current_scope}>
  <.header>
    Edit Note {@note.id}
    <:subtitle>Use this form to manage note records in your database.</:subtitle>
  </.header>

  <.note_form changeset={@changeset} action={~p"/notes/#{@note}"} return_to={~p"/notes"} />
</Layouts.app>
